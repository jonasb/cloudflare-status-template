# Replace name with your own app name
name = "cloudflare-status-template"
main = "src/index.tsx"
compatibility_date = "2023-11-21"

[[d1_databases]]
binding = "DB"
# Replace with your own database name and ID
database_name = ""
database_id = ""

[site]
bucket = "./assets"

[triggers]
# Configure the CRON schedules, ensure the probes in src/probes.ts are configured accordingly
crons = [ "0 * * * *" ]
